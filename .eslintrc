{
	"env": {
		"browser": true
	},
	"extends": ["airbnb-typescript"],
	"parser": "@typescript-eslint/parser",
	"parserOptions": {
		"project": "tsconfig.json",
		"sourceType": "module"
	},
	"ignorePatterns": ["webpack/*"],
	"rules": {
		"@typescript-eslint/no-explicit-any": "off",
		"react/display-name": "off"
	},
	"settings": {
		"react": {
			"version": "detect"
		}
	}
}